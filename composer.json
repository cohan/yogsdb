{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "alaouy/youtube": "1.*",
        "andreasindal/laravel-markdown": "^1.1",
        "aws/aws-sdk-php": "3.30.4",
        "babenkoivan/scout-elasticsearch-driver": "^4.0",
        "dborsatto/php-giantbomb": "^1.0",
        "doctrine/dbal": "^2.6",
        "elasticsearch/elasticsearch": "7.3.*",
        "fabpot/goutte": "^3.2",
        "fideloper/proxy": "^4.0",
        "giantbomb/giantbomb-php-api": "dev-master",
        "honeybadger-io/honeybadger-laravel": "^1.5",
        "laracasts/generators": "master@dev",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.0",
        "laravel/scout": "^7.2",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mantas-done/subtitles": "^0.3.3",
        "predis/predis": "^1.1",
        "shipping-docker/vessel": "^3.2",
        "simplesoftwareio/simple-cache": "^1.0",
        "spatie/laravel-backup": "^5.7",
        "spatie/laravel-permission": "^2.12",
        "spatie/laravel-sitemap": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
