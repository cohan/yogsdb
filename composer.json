{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "alaouy/youtube": "1.*",
        "andreasindal/laravel-markdown": "^1.1",
        "aws/aws-sdk-php": "3.30.4",
        "dborsatto/php-giantbomb": "^1.0",
        "fabpot/goutte": "^3.2",
        "giantbomb/giantbomb-php-api": "dev-master",
        "laracasts/generators": "master@dev",
        "laravel/framework": "5.4.*",
        "laravel/scout": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mantas-done/subtitles": "^0.3.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-cache": "^1.0",
        "spatie/laravel-backup": "^4.18",
        "spatie/laravel-permission": "^1.11",
        "spatie/laravel-sitemap": "^2.2",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
